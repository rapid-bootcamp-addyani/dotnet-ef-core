@model DotnetEfCore.DataContext.LoanInventoryEntity

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Detail LoanInventory</h2>
<table class="table table-bordered">
    <tbody>
        <tr>
            <th>ID</th>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <th>User Name</th>
            <td>@Model.NameUser</td>
        </tr>
        <tr>
            <th>Detail</th>
            <td>@Model.Detail</td>
        </tr>
        <tr>
            <th>Name Admin</th>
            <td>@Model.NameAdmin</td>
        </tr>
        <tr>
            <th>Status</th>
            <td>@Model.Status</td>
        </tr>
        <tr>
            <th>Date</th>
            <td>@Model.Date</td>
        </tr>

    </tbody>
</table>

<a class="btn btn-sm btn-primary" asp-area="" asp-controller="LoanInventory" asp-action="GetAll">Back</a>
